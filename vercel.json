{
  "version": 2,
  "builds": [
    {
      "src": "Backend/src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {"distDir": "Frontend/dist"}
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/dist/$1"
    }
  ]
}